    "tslint-react": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/meno"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/handshake-foundation/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
