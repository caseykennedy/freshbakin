    "coingecko-api": "^1.0.10",
    "framer-motion": "^4.1.4",
    "gatsby": "^3.3.0",
    "gatsby-image": "^3.9.0",
    "gatsby-plugin-google-tagmanager": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
