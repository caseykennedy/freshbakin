    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "swiper": "^6.5.7",
    "theme-ui": "^0.10.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
